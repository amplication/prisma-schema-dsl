{
  "name": "@pmaltese/prisma-schema-dsl",
  "version": "3.2.0",
  "description": "JavaScript interface for Prisma Schema DSL",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write src",
    "check-format": "prettier --check src",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "prisma",
    "schema",
    "dsl",
    "javascript",
    "ast",
    "generator"
  ],
  "author": {
    "name": "Iddan Aaronsohn"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/internals": "^4.12.0",
    "lodash.isempty": "^4.4.0"
  },
  "devDependencies": {
    "@pmaltese/prisma-schema-dsl-types": "^1.2.0",
    "@types/jest": "^26.0.24",
    "@types/lodash.isempty": "^4.4.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
